{
    "version": 2,
    "builds": [

        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [

        {
            "src": "/api/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "hooks": {
        "build": {
            "command": "npm run vercel-build"
        }
    }
}
